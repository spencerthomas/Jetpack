{
  "name": "@jetpack/orchestrator",
  "version": "0.1.0",
  "description": "Multi-agent orchestration engine for Jetpack swarm development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "jetpack",
    "orchestrator",
    "multi-agent",
    "swarm",
    "ai-agents",
    "coordination"
  ],
  "author": "Tom Spencer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spencerthomas/Jetpack.git",
    "directory": "packages/orchestrator"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@jetpack/shared": "workspace:*",
    "@jetpack/beads-adapter": "workspace:*",
    "@jetpack/mcp-mail-adapter": "workspace:*",
    "@jetpack/cass-adapter": "workspace:*",
    "@jetpack/supervisor": "workspace:*",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2",
    "vitest": "^1.6.0"
  }
}
