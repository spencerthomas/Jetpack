{
  "name": "@jetpack-agent/coordinator",
  "version": "0.1.0",
  "description": "Swarm coordinator for Jetpack - manages agent lifecycle and work distribution",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jetpack-agent/data": "workspace:*",
    "@jetpack-agent/agent-harness": "workspace:*",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.8.3",
    "vitest": "^2.1.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
