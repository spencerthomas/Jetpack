{
  "name": "@jetpack-agent/shared",
  "version": "0.1.1",
  "description": "Shared types and utilities for Jetpack multi-agent orchestration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./sync": {
      "types": "./dist/sync/index.d.ts",
      "default": "./dist/sync/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "jetpack",
    "multi-agent",
    "ai",
    "orchestration",
    "types"
  ],
  "author": "Tom Spencer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spencerthomas/Jetpack.git",
    "directory": "packages/shared"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "typescript": "^5.7.2",
    "vitest": "^1.6.0"
  }
}
